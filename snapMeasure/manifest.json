{
  "name": "SnapGuide",
  "description": "Chrome extension for measuring elements on page",
  "version": "0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Turn SnapGuide on"
  },
  "manifest_version": 2
}
